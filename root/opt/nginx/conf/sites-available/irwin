upstream irwin_node_io {
  server 127.0.0.1:2883;
}

server {
  listen   2884;
  server_name  www.womack.io;
  rewrite ^/(.*) http://womack.io/$1 permanent; 
}
 

# the nginx server instance
server {
    listen 2884;
    server_name womack.io;

    access_log /root/public/nblgstr.com/log/irwin.access.log;
    error_log /root/public/nblgstr.com/log/irwin.error.log;

    # pass the request to the node.js server with the correct headers and much more can be added, see nginx config options
    location / {
      proxy_set_header X-Real-IP $remote_addr;
      proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
      proxy_set_header Host $http_host;
      proxy_set_header X-NginX-Proxy true;

      proxy_pass http://irwin_node_io/;
      proxy_redirect off;
    }
 }
